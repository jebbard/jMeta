<?xml version="1.0" encoding="UTF-8"?>
<extp:ExtensionPointsConfiguration
	version="1.0"
	xmlns:extp="www.easytag.de/XMLSchema_v1_0"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="www.easytag.de/XMLSchema_v1_0 ExtensionPointsConfiguration.xsd">

   <!-- The (absolute or relative) path locations to the extension bundles -->
	<ExtensionBundleSearchPath>
		<Path>./ext</Path>
	</ExtensionBundleSearchPath>

   <!-- List of all supported extension points -->
	<ExtensionPoints>
		<ExtensionPoint id="DataFormatsExtension">
			<ExtensionInterface>de.je.jmeta.dataformats.export.IDataFormatsExtension</ExtensionInterface>
			<ComponentName>DataFormats</ComponentName>
		</ExtensionPoint>
		<ExtensionPoint id="DataBlocksExtension">
			<ExtensionInterface>de.je.jmeta.datablocks.export.IDataBlocksExtension</ExtensionInterface>
			<ComponentName>DataBlocks</ComponentName>
		</ExtensionPoint>
	</ExtensionPoints>

</extp:ExtensionPointsConfiguration>