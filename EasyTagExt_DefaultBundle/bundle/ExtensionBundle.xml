<?xml version="1.0" encoding="UTF-8"?>
<extb:ExtensionBundle
	name="defJMeta"
	version="1.0"
	xmlns:extb="www.easytag.de/XMLSchema_v1_0"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="www.easytag.de/XMLSchema_v1_0 ExtensionBundleConfiguration.xsd">

   <!-- The basic properties of the bundle -->
	<BundleProperties>
		<Name>Default jMeta extension bundle</Name>
		<Description>This bundle contains all of the default extensions for jMeta.</Description>
		<Author>Jens Ebert</Author>
		<Date>2011-05-05</Date>
		<Version>1.0</Version>
		<Type>DEFAULT</Type>
	</BundleProperties>

   <!-- List of all extensions in the bundle -->
	<BundledExtensions>
   	<!-- DataFormats extension for ID3v1 and ID3v1.1 -->
		<Extension id="DefaultjMetaID3v1FormatExtension">
			<ExtensionPointId>DataFormatsExtension</ExtensionPointId>
			<Provider>de.je.jmeta.defext.dataformats.impl.ID3v1DummyDataFormatExtension</Provider>
			<RelativePath>../target/classes</RelativePath>
		</Extension>
   	<!-- DataFormats extension for ID3v2.3 -->
		<Extension id="DefaultjMetaID3v23FormatExtension">
			<ExtensionPointId>DataFormatsExtension</ExtensionPointId>
			<Provider>de.je.jmeta.defext.dataformats.impl.ID3v23DummyDataFormatExtension</Provider>
			<RelativePath>../target/classes</RelativePath>
		</Extension>
   	<!-- DataFormats extension for MP3 -->
		<Extension id="DefaultjMetaMP3FormatsExtension">
			<ExtensionPointId>DataFormatsExtension</ExtensionPointId>
			<Provider>de.je.jmeta.defext.dataformats.impl.MP3DummyDataFormatExtension</Provider>
			<RelativePath>../target/classes</RelativePath>
		</Extension>
   	<!-- DataFormats extension for APEv2 -->
		<Extension id="DefaultjMetaAPEv2FormatsExtension">
			<ExtensionPointId>DataFormatsExtension</ExtensionPointId>
			<Provider>de.je.jmeta.defext.dataformats.impl.APEv2DummyDataFormatExtension</Provider>
			<RelativePath>../target/classes</RelativePath>
		</Extension>
   	<!-- DataFormats extension for Lyrics3v2 -->
		<Extension id="DefaultjMetaLyrics3v2FormatsExtension">
			<ExtensionPointId>DataFormatsExtension</ExtensionPointId>
			<Provider>de.je.jmeta.defext.dataformats.impl.Lyrics3v2DummyDataFormatExtension</Provider>
			<RelativePath>../target/classes</RelativePath>
		</Extension>
   	<!-- DataFormats extension for Ogg -->
		<Extension id="DefaultjMetaOggFormatsExtension">
			<ExtensionPointId>DataFormatsExtension</ExtensionPointId>
			<Provider>de.je.jmeta.defext.dataformats.impl.OggDummyDataFormatExtension</Provider>
			<RelativePath>../target/classes</RelativePath>
		</Extension>

   	<!-- DataBlocks extension for ID3v1 and ID3v1.1 -->
		<Extension id="DefaultjMetaID3v1BlocksExtension">
			<ExtensionPointId>DataBlocksExtension</ExtensionPointId>
			<Provider>de.je.jmeta.defext.datablocks.impl.ID3v1DataBlocksExtension</Provider>
			<RelativePath>../target/classes</RelativePath>
		</Extension>
   	<!-- DataBlocks extension for ID3v2.3 -->
		<Extension id="DefaultjMetaID3v23BlocksExtension">
			<ExtensionPointId>DataBlocksExtension</ExtensionPointId>
			<Provider>de.je.jmeta.defext.datablocks.impl.ID3v23DataBlocksExtension</Provider>
			<RelativePath>../target/classes</RelativePath>
		</Extension>
   	<!-- DataBlocks extension for MP3 -->
		<Extension id="DefaultjMetaMP3BlocksExtension">
			<ExtensionPointId>DataBlocksExtension</ExtensionPointId>
			<Provider>de.je.jmeta.defext.datablocks.impl.MP3DataBlocksExtension</Provider>
			<RelativePath>../target/classes</RelativePath>
		</Extension>
   	<!-- DataBlocks extension for APEv2 -->
		<Extension id="DefaultjMetaAPEv2BlocksExtension">
			<ExtensionPointId>DataBlocksExtension</ExtensionPointId>
			<Provider>de.je.jmeta.defext.datablocks.impl.APEv2DataBlocksExtension</Provider>
			<RelativePath>../target/classes</RelativePath>
		</Extension>
   	<!-- DataBlocks extension for Lyrics3v2 -->
		<Extension id="DefaultjMetaLyrics3v2BlocksExtension">
			<ExtensionPointId>DataBlocksExtension</ExtensionPointId>
			<Provider>de.je.jmeta.defext.datablocks.impl.Lyrics3v2DataBlocksExtension</Provider>
			<RelativePath>../target/classes</RelativePath>
		</Extension>
   	<!-- DataBlocks extension for Ogg -->
		<Extension id="DefaultjMetaOggBlocksExtension">
			<ExtensionPointId>DataBlocksExtension</ExtensionPointId>
			<Provider>de.je.jmeta.defext.datablocks.impl.OggDataBlocksExtension</Provider>
			<RelativePath>../target/classes</RelativePath>
		</Extension>
	</BundledExtensions>

</extb:ExtensionBundle>